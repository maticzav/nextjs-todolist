{
  "name": "nextjs-todolist",
  "version": "1.0.0",
  "main": "server.js",
  "repository": {},
  "license": "MIT",
  "xo": {
    "extends": "xo-react",
    "space": true,
    "semicolon": false,
    "rules": {
      "react/react-in-jsx-scope": 0,
      "react/jsx-pascal-case": 0,
      "object-curly-spacing": 0,
      "react/jsx-curly-spacing": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "import/named": 0,
      "react/prop-types": 0,
      "react/forbid-component-props": 0
    }
  },
  "now": {
    "alias": "todolist.now.sh"
  },
  "dependencies": {
    "apollo-client": "^0.8.5",
    "express-session": "^1.15.1",
    "graphql": "^0.9.1",
    "next": "^1.2.3",
    "nprogress": "^0.2.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "react": "^15.4.2",
    "react-apollo": "^0.10.1",
    "react-dom": "^15.4.2",
    "react-no-ssr": "^1.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "eslint-config-xo": "^0.17.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.9.0",
    "xo": "^0.17.1"
  }
}
